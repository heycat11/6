#gcd
def euclid(m,n):
    if m<n:
        (m,n)=(n,m)
    while n!=0:
        r=m%n
        m=n
        n=r
    return m

def consecutive_integer_checking(m,n):
    t=min(m,n)
    while t!=0:
        if m%t==0 and n%t==0:
            return t
        t=t-1

a=int(input("Enter the value of m: "))
b=int(input("Enter the value of n: "))
print("Using euclid: ",euclid(a,b))
print("Using consecutive integer checking: ",consecutive_integer_checking(a,b))
